public class Store {
  // instance fields
  String productType;
  int inventoryCount;
  double inventoryPrice;
  // constructor method
  public Store(String product) {
    productType = product;
  public Store(int count)  {
    inventoryCount = count;
  public Store (double price)   { 
    inventoryPrice = price;

  }
  }
  }
  
  // main method
  public static void main(String[] args) {
    
  }
}
